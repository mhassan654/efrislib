{
    "name": "sniper/efris-lib",
    "description": "PHP Library for EFRIS system to system integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sniper\\EfrisLib\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Omega Jean Emmy",
            "email": "omegaemmy@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "ext-openssl": "*",
        "ext-dom": "*",
        "tcdent/php-restclient": "0.1.8.2",
        "ext-curl": "*",
        "symfony/serializer": "v6.4.7",
        "symfony/property-access": "v6.4.7",
        "symfony/property-info": "v6.4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.18"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sniper\\EfrisLib\\Providers\\SerializerServiceProvider"
            ]
        }
    }
}
